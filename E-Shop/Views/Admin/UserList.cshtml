@model List<EntityLayer.Entities.User>
@{
    ViewBag.Title = "UserList";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<h2>KULLANICILAR</h2>

<table class="table table-bordered">
    <tr>
        <th>Ad Soyad</th>
        <th>Email</th>
        <th>Kullanici Adi</th>
        <th>Sifre</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Name @item.SurName</td>
            <td>@item.Email</td>
            <td>@item.UserName</td>
            <td>@item.Password</td>
        </tr>
    }
</table>

